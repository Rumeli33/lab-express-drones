<h2>Available drones</h2>
{{#each drones}}
<section class ="drone-summary">
    <h2>{{this.name}}</h2>
 
<p>
     No.Of propellers: {{this.propellers}} <br>
    Maximum Speed : {{this.maxSpeed}} mtr/sec<br>
    
</p>
<a href="/drones/{{this._id}}/edit">Edit</a>
<p>
    <form action="/drones/{{_id}}/delete" method ="post"> 
        <button>Delete this drone</button>
        </form>
</p>
</section>

{{/each}}